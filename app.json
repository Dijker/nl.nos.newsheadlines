{
    "id": "nl.webchitect.newsheadlines",
    "name": {
        "en": "News headlines"
    },
    "version": "0.0.1",
    "compatibility": "0.x || 1.x",
    "author": {
        "name": "Christian Versloot",
        "email": "mail@christianversloot.nl"
    },
    "images": {
        "large": "./assets/images/large.jpg",
